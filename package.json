{
  "name": "repositories",
  "version": "1.0.0",
  "description": "Repositories management",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon dist/server",
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "author": "David Leonardo Dallos Parra",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.3",
    "@types/pg": "^8.6.5",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jest": "^28.1.3",
    "pg": "^8.7.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5"
  }
}
